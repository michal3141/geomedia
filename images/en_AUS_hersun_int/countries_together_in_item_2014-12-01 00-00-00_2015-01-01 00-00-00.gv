// Countries together in item graph
graph {
	graph [bgcolor="#888888" fontcolor=white fontsize=12 label="Graph" outputorder=edgesfirst overlap=prism]
	node [color=white fillcolor="#006699" fontcolor=white fontname=Helvetica shape=hexagon style=filled]
	edge [arrowhead=open color=black fontcolor=white fontname=Courier fontsize=12]
		AUS [label=AUS]
		SYR [label=SYR]
		USA [label=USA]
		KOR [label=KOR]
		JPN [label=JPN]
		NRU [label=NRU]
		UKR [label=UKR]
		PHL [label=PHL]
		MYS [label=MYS]
		FJI [label=FJI]
		"CCK " [label="CCK "]
		IRQ [label=IRQ]
		IRN [label=IRN]
		DEU [label=DEU]
		AFG [label=AFG]
		PAK [label=PAK]
		CAN [label=CAN]
		PER [label=PER]
		EGY [label=EGY]
		CHN [label=CHN]
		LBN [label=LBN]
		GBR [label=GBR]
			AUS -- UKR [weight=2]
			AUS -- IRN [weight=1]
			AUS -- PAK [weight=1]
			MYS -- AUS [weight=1]
			AUS -- USA [weight=1]
			AUS -- LBN [weight=1]
}