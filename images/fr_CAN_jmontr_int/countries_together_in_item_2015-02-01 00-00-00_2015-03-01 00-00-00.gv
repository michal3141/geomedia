// Countries together in item graph
graph {
	graph [bgcolor="#888888" fontcolor=white fontsize=12 label="Graph" outputorder=edgesfirst overlap=prism]
	node [color=white fillcolor="#006699" fontcolor=white fontname=Helvetica shape=hexagon style=filled]
	edge [arrowhead=open color=black fontcolor=white fontname=Courier fontsize=12]
		CHE [label=CHE]
		USA [label=USA]
		RUS [label=RUS]
		TCD [label=TCD]
		NER [label=NER]
		LBY [label=LBY]
		IRQ [label=IRQ]
		CAN [label=CAN]
		SAU [label=SAU]
		DNK [label=DNK]
		ARG [label=ARG]
		ISR [label=ISR]
		PSE [label=PSE]
		UKR [label=UKR]
		KOR [label=KOR]
		CUB [label=CUB]
		NZL [label=NZL]
		SYR [label=SYR]
		FRA [label=FRA]
		GRC [label=GRC]
			CAN -- SAU [weight=1]
			NER -- TCD [weight=1]
			CAN -- CUB [weight=1]
			ISR -- PSE [weight=1]
}