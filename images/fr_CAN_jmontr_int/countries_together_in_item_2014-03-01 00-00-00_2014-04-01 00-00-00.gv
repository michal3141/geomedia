		CAN [label=CAN]
		EGY [label=EGY]
		JPN [label=JPN]
		DNK [label=DNK]
		TUN [label=TUN]
		CHN [label=CHN]
		PAN [label=PAN]
		SYR [label=SYR]
		MKD [label=MKD]
		PAK [label=PAK]
		MYS [label=MYS]
		IND [label=IND]
